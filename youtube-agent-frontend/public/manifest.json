{
  "manifest_version": 3,
  "name": "Youtube Agent 2",
  "description": "Test your React app as a Chrome extension.",
  "version": "1.0",
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "favicon.ico"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"]
}

